        &quot;out/vrecovery.fex&quot;             extracted
        &quot;out/voem.fex&quot;          extracted
        &quot;out/boot0.bin&quot;         decrypted
        &quot;out/boot1.bin&quot;         decrypted
        &quot;eFex//usb//tools.fex&quot;          extracted
